{
    "name": "abby/backpack-knowledgebase",
    "type": "library",
    "description": "An admin panel for a knowledgebase, using Backpack\\CRUD on Laravel 5.",
    "keywords": [
        "backpack",
        "knowledgebase"
    ],
    "homepage": "https://github.com/AbbyJanke/Backpack-Knowledgebase",
    "license": "MIT",
    "authors": [
        {
            "name": "Abby Janke",
            "email": "me@abbyjanke.com",
            "homepage": "http://abbyjanke.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "backpack/crud": "^3.0.6",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "cviebrock/eloquent-sluggable": "^4.0",
        "illuminate/support": "~5.1",
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Abby\\Knowledgebase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abby\\Knowledgebase\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
